---
import Layout from "../layouts/Layout.astro";
import Header from "../components/common/Header.astro";
import Form from "../components/contact/Form.astro";
import Values from "../components/contact/Values.astro";
import Email from "../components/contact/Email.astro";
import language from "../i18n/it";
const t = language();
---

<Layout t={t} meta={t.contact.meta}>
  <Header header={t.contact.header} />
  <Form form={t.contact.form} />
  <Values values={t.contact.values} />
  <Email email={t.contact.email} />
</Layout>

<script src="../scripts/form.js"></script>
