---
import Layout from "../../layouts/Layout.astro";
import Header from "../../components/contact/Header.astro";
import Form from "../../components/contact/Form.astro";
import Contacts from "../../components/contact/Contacts.astro";
import translation from "../../i18n/en";
const t = translation("/en");
---

<Layout t={t} meta={t.contact.meta}>
  <Header badge={t.contact.badge} title={t.contact.title} description={t.contact.description} />
  <Form formData={t.contact.form} />
  <Contacts alternativeData={t.contact.alternative} />
</Layout>
