---
import Layout from "../../layouts/Layout.astro";
import Header from "../../components/contact/Header.astro";
import Form from "../../components/contact/Form.astro";
import Email from "../../components/contact/Email.astro";
import language from "../../i18n/en";
const t = language("/en");
---

<Layout t={t} meta={t.contact.meta}>
  <Header header={t.contact.header} />
  <Form form={t.contact.form} />
  <Email email={t.contact.email} />
</Layout>
