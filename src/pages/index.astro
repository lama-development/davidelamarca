---
import Layout from "../layouts/Layout.astro";
import Hero from "../components/home/Hero.astro";
import Services from "../components/home/Services.astro";
import Projects from "../components/home/Projects.astro";
import FAQ from "../components/home/FAQ.astro";
import Banner from "../components/home/Banner.astro";
import translation from "../i18n/it";
const t = translation();
---

<Layout t={t} meta={t.home.meta}>
  <Hero {...t.home.hero} />
  <Services {...t.home.services} />
  <Projects {...t.home.projects} />
  <FAQ {...t.home.faq} />
  <Banner {...t.home.banner} />
</Layout>
