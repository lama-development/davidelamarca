---
import Layout from "../layouts/Layout.astro";
import Header from "../components/common/Header.astro";
import ProjectCards from "../components/projects/ProjectCards.astro";
import Testimonials from "../components/projects/Testimonials.astro";
import language from "../i18n/it";
const t = language();
---

<Layout t={t} meta={t.projects.meta}>
  <Header header={t.projects.header} />
  <ProjectCards projectCard={t.projects.ProjectCards} isHomePage={false} />
  <Testimonials testimonials={t.projects.testimonials} />
</Layout>

<script src="../scripts/testimonials.js"></script>
